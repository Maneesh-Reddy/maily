{
    "manifest_version": 3,
    "name": "Email Priority Manager",
    "version": "1.0",
    "description": "Fetch and prioritize your emails easily.",
    "permissions": ["identity", "storage"],
    "host_permissions": [
      "https://www.googleapis.com/*",
      "https://www.googleapis.com/auth/gmail.readonly*",
      "https://gmail.googleapis.com/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "oauth2": {
      "client_id": "501169230895-fpmi5kthos2kvve0ih02kuig753sdppo.apps.googleusercontent.com",
      "scopes": [
        "https://www.googleapis.com/auth/gmail.readonly",
        "https://www.googleapis.com/auth/userinfo.email"
      ]
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  